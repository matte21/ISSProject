digraph finite_state_machine {
	rankdir=LR
	size="8,5"
	
	init [ color = red ][ fontcolor = red ][ penwidth = 3 ]
	launch_job [ color = blue ][ fontcolor = blue ][ penwidth = 3 ]
	
	init -> launch_job [ label = "switch" ][ color = black ][ fontcolor = black ]
}
